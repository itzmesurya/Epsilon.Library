{
    "ace": {
        "ep-text": "<ep-text-ace options=\"options\" model-var=\"model[options.key]\"></ep-text-ace>"
    },
    "materiallab": {
        "ep-text": "<ep-text-ml config=\"options\" model-var=\"model[options.key]\" ></ep-text-ml>",
        "ep-dropdown-menu": "<ep-text-ml config=\"options\" model-var=\"model[options.key]\" ></ep-text-ml>"
    },
    "custom": {
        "ep-text": "<div class=\"form-group\"><label class=\"col-sm-2 control-label no-padding-right\">{{to.label}}</label><div class=\"col-sm-10\"><input id=\"{{options.id}}\" type=\"{{to.validationType}}\" name=\"{{options.name}}\" placeholder=\"Enter your {{to.label }}\" ng-required={{to.required}} maxlength=\"{{to.maxlength}}\" minlength=\"{{to.minlength}}\" ng-model=\"model[options.key]\" class=\"form-control\" data-rule-email=\"true\" ><span id=\"{{options.id}}-error\" class=\"help-block\" style=\"display: none;\"></span></div> </div>"
    }
}
